            let a = parseInt(values.strength_art) || 0;
            let m = parseInt(values.strength_misc) || 0;
            let t = parseInt(values.strength_temp) || 0;   
            setAttrs({ 
                strength : r+c+g+a+m+t
            });
        });
        });

