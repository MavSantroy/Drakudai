                                <td class="skill-bonus"><button class="blank-roll-button" type="roll" value="&{template:default} {{name=Melee}} {{roll=[[1d20+@{melee}]]}}" />+<span name="attr_melee" /></button></td>
                                <td class="skill-name"><span class="subheading">Pilot</span></td>
                                <td class="skill-bonus"><button class="blank-roll-button" type="roll" value="&{template:default} {{name=Pilot}} {{roll=[[1d20+@{pilot}]]}}" />+<span name="attr_pilot" /></button></td>
                            </tr>
                            <tr>
                                <td class="skill-name"><span class="subheading">Sleight of hand</span></td>
                                <td class="skill-bonus"><button class="blank-roll-button" type="roll" value="&{template:default} {{name=Sleight of hand}} {{roll=[[1d20+@{sleightOfHand}]]}}" />+<span name="attr_sleightOfHand" /></button></td>
                                <td class="skill-name"><span class="subheading">Stealth</span></td>
                                <td class="skill-bonus"><button class="blank-roll-button" type="roll" value="&{template:default} {{name=Stealth}} {{roll=[[1d20+@{stealth}]]}}" />+<span name="attr_stealth" /></button></td>                        
                                <td class="skill-name"><span class="subheading">Survival</span></td>
                                <td class="skill-bonus"><button class="blank-roll-button" type="roll" value="&{template:default} {{name=Survival}} {{roll=[[1d20+@{survival}]]}}" />+<span name="attr_survival" /></button></td>
                            </tr>
                        </table>
                </div>        
        </div>
    
    </div>
    
    
    
    <!-- ROLL TEMPLATE -->
    
    <rolltemplate class="sheet-rolltemplate-custom">
    <div class="sheet-container sheet-color-{{color}}">
    <div class="sheet-header">
      {{#title}}<div class="sheet-title">{{title}}</div>{{/title}}
      {{#subtitle}}<div class="sheet-subtitle">{{subtitle}}</div>{{/subtitle}}
      {{#subtitle2}}<div class="sheet-subtitle2">{{subtitle2}}</div>{{/subtitle2}}
    </div>
    <div class="sheet-content">
      {{#allprops() title subtitle subtitle2 desc color}}
      <div class="sheet-key">{{key}}</div>
      <div class="sheet-value">{{value}}</div>
      {{/allprops() title subtitle subtitle2 desc color}}
      {{#desc}}<div class="sheet-desc">{{desc}}</div>{{/desc}}
    </div>
    </div>
    </rolltemplate>
    
    
    <!-- END OF HTML -->
    
    
    
    
    
    
    <script type="text/worker">
    
    
    
    /* --------------------------- FUNCTIONS -----------------------------------------------
    These functions can be called by the watchers (further down) or each other. 
    */
    
    
    const dice_values = ['1', '1D2', '1D4', '1D6', '1D8', '1D10', '1D12', '2D6', '2D8', '3d6','2D10', '2D12', '3d8', '4D6', '3d10','5d6','4D8', '3d12', '6dd', '4D10', '7d6', '4d12','6D8', '8D6', '5d10', '7d8', '5D12', '6d10', '8D8', '7D10', '6D12', '8d10', '7d12', '9D10', '8d12', '10d10', '9d12', '11D10', '10D12', '12D10', '12d12', '8d20', '10d20', '12d20', '20d20'];
    const stat_names = ['sub_skill_attack_dice_total'];
    const name_end = 'total'; //if you want to use a different name end, change it here.
    stat_names.forEach(stat => {
        on(`change:${stat}`, () => {
            getAttrs(['stat', 'sub_skill_dice_cost', 'sub_skill_dice_paid', 'sub_skill_attack_dice_total'], v => {
                let cost = parseInt(values.sub_skill_attack_dice_cost) || 0;
                let paid = parseInt(values.sub_skill_attack_dice_paid) || 0;    
                let dice_level = cost/paid;
                
                const score = +v[stat] || 0;
                const dice = dice_values[Math.max(0, Math.min(dice_values.length -1, score))];
                setAttrs({
                    [`${stat}_${name_end}`]: dice
                });
            });
        });
    });
    
    
    function updatePointsSpent() {
    /*
        This function will be called upon by many different watchers, such as 
        when you invest in a skill or invest in a primary attribute. 
        As such, rather than duplicate similar code in each case, it is 
        easily referenced as a function instead. 
        
        Input: nothing
        Return: nothing
    */ 
    attributes = ['strengthPoints', 'dexterityPoints', 'constitutionPoints', 'intelligencePoints', 'wisdomPoints', 'charismaPoints',
        'biologyPoints', 'businessPoints', 'chemistryPoints', 'engineeringPoints', 'tronicsPoints', 'lawPoints', 'physicsPoints', 'programmingPoints', 
        'acrobaticsPoints', 'athleticsPoints', 'awarenessPoints', 'barterPoints', 'charmPoints', 'diplomacyPoints', 'fighterPilotPoints', 
        'focusPoints', 'forgeryPoints', 'gatherInfoPoints', 'insightPoints', 'intimidatePoints', 'investigationPoints', 'medicPoints', 'meleePoints', 'performPoints', 
        'pilotPoints', 'sleightOfHandPoints', 'stealthPoints', 'survivalPoints'];
    getAttrs(attributes, function(values) { 
    let total = 0;
    attributes.forEach(attribute =>{  total += parseInt(values[attribute]) || 0;   });
    setAttrs({ totalPoints : total });
    });
    }
    
    function updateActionPoints() {
    getAttrs(['dexterity', 'intelligence', 'life_energy','raw_energy'], function(values) {
        let dex = parseInt(values.dexterity) / 5 || 1;
        let int = parseInt(values.intelligence) / 5 || 1;
        let le = parseInt(values.life_energy) || 1;
        let re = Math.floor(parseInt(values.raw_energy) / 100) || 1        
        let points = dex + int + re ;        
        setAttrs({ 
            actionPoints : points 
        });
    });
    }
    
    // function updateActionPoints() {
    //  getAttrs(['dexterity', 'intelligence', 'life_energy'], function(values) {
    //      let dex = parseInt(values.dexterity) || 1;
    //      let int = parseInt(values.intelligence) || 1;
    //      let le = parseInt(values.life_energy) || 1;
    //      let re = Math.floor((le - 100) / 10)+1;
    //      let points = dex + int + re;
    //      let progressionList=[];
    //      for (i = 1; i < 40; i++) {
    //          progressionList = progressionList.concat(Math.pow(2, i-1));
    //      }
    //      let skillLevelBought = buySkillLevel(progressionList, points);
    //      let result = Math.max(skillLevelBought, 1);
    //      setAttrs({ 
    //          actionPoints : result 
    //      });
    //  });
    // }
    
    function updateMovement() {
    getAttrs(['dexterity','movementRacial']), function(values) {
        let dex = parseInt(values.dexterity) || 0;
        let move = parseInt(values.movementRacial) || 0;
        setAttrs({ 
            movement : move + dex*5 
        })
    }
    }
    
    function updateDefense() {
    getAttrs(['strength', 'dexterity', 'constitution', 'wisdom', 'armorPenetrationPlus', 'shieldsPenetrationPlus', 'racialHPBonus', 'armor_defense', 'shields_defense', 'life_energy', 'psi_deflection', 'psi_domination', 'psi_clairvoyance'], function(values) {
        /*
            DEFENSE
            Ranged Def      10+Dex+Armor Def + Shield Def+Deflection+Misc
            Melee Def       Roll off + Melee Skill + Misc + Deflection
            Mental Def      20+Wis+Deflection+RE+Misc
            CDS             _"10+Tech Level"__
            
            HEALTH
            HP          Race Base Bonus + Strength x Constitution
            P+          P+ = Floor( Con / 10)
            Regen       Floor(CON/5) / day
        */
    
        let str = parseInt(values.strength) || 0;
        let dex = parseInt(values.dexterity) || 0;
        let con = parseInt(values.constitution) || 0;
        let wis = parseInt(values.wisdom) || 0;
        
        let le = parseInt(values.life_energy) || 0;
        let re = Math.floor(le - 100)/10 || 0;
        
        let techLevel = 0;
        
        let racialHPBonus = parseInt(values.racialHPBonus) || 0;
        
        let basePenetrationPlus = Math.floor(con/10) || 0;
        let armorPenetrationPlus = parseInt(values.armorPenetrationPlus) || 0;
        let shieldsPenetrationPlus = parseInt(values.shieldsPenetrationPlus) || 0;
        let armor_def = parseInt(values.armor_defense) || 0;
        let shields_def = parseInt(values.shields_defense) || 0;        
    
        // Set Attributes based on aliases above DEFENSE
        //setAttrs({ ranged_defense : 10 + dex + armor_def + shields_def + psi_deflection });
    
        // setAttrs({ mental_defense : 10 + wis + psi_dom + psi_cla + re/10 });
    
        // HEALTH
        setAttrs({ health_max : racialHPBonus + str*con });
        setAttrs({ penetrationPlus : basePenetrationPlus });
        setAttrs({ regen : Math.floor(con/5) });
        
    });
    }
    
    function buySkillLevel(progressionList, points) {
    let cont = true;
    let iterator = 0;
    let skillLevelBought = 0;
    let p = points;
    
    while (cont) {
        if (iterator >= progressionList.length) {
            iterator = 0;
        }
        else if (p >= progressionList[iterator]) {
            p = p - progressionList[iterator];
            skillLevelBought = skillLevelBought +1;
            iterator = iterator +1;
        }
        else {
            cont = false;
        }
    }
    return skillLevelBought;
    }
    
    /* --------------------------- WATCHERS -----------------------------------------------
    These guys look for changes in attributes and executes 
    code when they notice a change. 
    */
    
    
    /*
    You could condense the primary attribute watchers into pone, however, 
    doing so would only save a few lines of code and come at the cost of having
    to include lots of various bloated code. 
    For example, if carry weight depends solely on strength, then it is 
    unnecessary to recalculate it every time I change my intelligence. 
    Thus why we have specific watchers for each primary attribute. 
    This also makes it easier to debug, since each attribute handles itself. 
    */
    
    
    on('change:sub_skill_dice_cost change:sub_skill_attack_dice_paid', function(eventInfo) {
    getAttrs(['sub_skill_attack_dice_cost', 'sub_skill_attack_dice_paid'], function(values) {       
        let cost = parseInt(values.sub_skill_attack_dice_cost)
        let paid = parseInt(values.sub_skill_attack_dice_paid)                
        setAttrs({ sub_skill_attack_dice_total : Math.floor(cost/paid) });
    });
    });
    
    
    on('change:strengthRacial change:strengthCybernetic change:strengthGenetic change:strengthVI change:strengthMisc change:strengthPoints', function(eventInfo) {
    getAttrs(['strengthRacial', 'strengthCybernetic', 'strengthGenetic', "strengthVI", "strengthMisc", 'strengthPoints'], function(values) {
        updatePointsSpent();
        let r = parseInt(values.strengthRacial) || 0;
        let c = parseInt(values.strengthCybernetic) || 0;
        let g = parseInt(values.strengthGenetic) || 0;
        let v = parseInt(values.strengthVI) || 0;
        let m = parseInt(values.strengthMisc) || 0;
        let p = parseInt(values.strengthPoints) || 0;
        let progressionList = [];
        for (i = 1; i < 100; i++) {
            progressionList = progressionList.concat(i);
        }
        let skillLevelBought = buySkillLevel(progressionList, p);
        setAttrs({ strength : r+c+g+v+m+skillLevelBought });
    });
    });
    
    
    
    on('change:dexterityRacial change:dexterityCybernetic change:dexterityGenetic change:dexterityVI change:dexterityMisc change:dexterityPoints', function(eventInfo) {
    getAttrs(['dexterityRacial', 'dexterityCybernetic', 'dexterityGenetic', "dexterityVI", "dexterityMisc", 'dexterityPoints'], function(values) {
        updatePointsSpent();
        let r = parseInt(values.dexterityRacial) || 0;
        let c = parseInt(values.dexterityCybernetic) || 0;
        let g = parseInt(values.dexterityGenetic) || 0;
        let v = parseInt(values.dexterityVI) || 0;
        let m = parseInt(values.dexterityMisc) || 0;
        let p = parseInt(values.dexterityPoints) || 0;
        let progressionList = [];
        for (i = 1; i < 100; i++) {
            progressionList = progressionList.concat(i);
        }
        let skillLevelBought = buySkillLevel(progressionList, p);
        setAttrs({ dexterity : r+c+g+v+m+skillLevelBought });
    });
    });
    
    
    on('change:constitutionRacial change:constitutionCybernetic change:constitutionGenetic change:constitutionVI change:constitutionMisc change:constitutionPoints', function(eventInfo) {
    getAttrs(['constitutionRacial', 'constitutionCybernetic', 'constitutionGenetic', "constitutionVI", "constitutionMisc", 'constitutionPoints'], function(values) {
        updatePointsSpent();
        let r = parseInt(values.constitutionRacial) || 0;
        let c = parseInt(values.constitutionCybernetic) || 0;
        let g = parseInt(values.constitutionGenetic) || 0;
        let v = parseInt(values.constitutionVI) || 0;
        let m = parseInt(values.constitutionMisc) || 0;
        let p = parseInt(values.constitutionPoints) || 0;
        let progressionList = [];
        for (i = 1; i < 100; i++) {
            progressionList = progressionList.concat(i);
        }
        let skillLevelBought = buySkillLevel(progressionList, p);
        setAttrs({ constitution : r+c+g+v+m+skillLevelBought });
    });
    });
    
    
    
    on('change:intelligenceRacial change:intelligenceCybernetic change:intelligenceGenetic change:intelligenceVI change:intelligenceMisc change:intelligencePoints', function(eventInfo) {
    getAttrs(['intelligenceRacial', 'intelligenceCybernetic', 'intelligenceGenetic', "intelligenceVI", "intelligenceMisc", 'intelligencePoints'], function(values) {
        updatePointsSpent();
        let r = parseInt(values.intelligenceRacial) || 0;
        let c = parseInt(values.intelligenceCybernetic) || 0;
        let g = parseInt(values.intelligenceGenetic) || 0;
        let v = parseInt(values.intelligenceVI) || 0;
        let m = parseInt(values.intelligenceMisc) || 0;
        let p = parseInt(values.intelligencePoints) || 0;
        let progressionList = [];
        for (i = 1; i < 100; i++) {
            progressionList = progressionList.concat(i);
        }
        let skillLevelBought = buySkillLevel(progressionList, p);
        setAttrs({ intelligence : r+c+g+v+m+skillLevelBought });
    });
    });
    
    
    
    on('change:wisdomRacial change:wisdomCybernetic change:wisdomGenetic change:wisdomVI change:wisdomMisc change:wisdomPoints', function(eventInfo) {
    getAttrs(['wisdomRacial', 'wisdomCybernetic', 'wisdomGenetic', "wisdomVI", "wisdomMisc", 'wisdomPoints'], function(values) {
        updatePointsSpent();
        let r = parseInt(values.wisdomRacial) || 0;
        let c = parseInt(values.wisdomCybernetic) || 0;
        let g = parseInt(values.wisdomGenetic) || 0;
        let v = parseInt(values.wisdomVI) || 0;
        let m = parseInt(values.wisdomMisc) || 0;
        let p = parseInt(values.wisdomPoints) || 0;
        let progressionList = [];
        for (i = 1; i < 100; i++) {
            progressionList = progressionList.concat(i);
        }
        let skillLevelBought = buySkillLevel(progressionList, p);
        setAttrs({ wisdom : r+c+g+v+m+skillLevelBought });
    });
    });
    
    
    
    on('change:charismaRacial change:charismaCybernetic change:charismaGenetic change:charismaVI change:charismaMisc change:charismaPoints', function(eventInfo) {
    getAttrs(['charismaRacial', 'charismaCybernetic', 'charismaGenetic', "charismaVI", "charismaMisc", 'charismaPoints'], function(values) {
        updatePointsSpent();
        let r = parseInt(values.charismaRacial) || 0;
        let c = parseInt(values.charismaCybernetic) || 0;
        let g = parseInt(values.charismaGenetic) || 0;
        let v = parseInt(values.charismaVI) || 0;
        let m = parseInt(values.charismaMisc) || 0;
        let p = parseInt(values.charismaPoints) || 0;
        let progressionList = [];
        for (i = 1; i < 100; i++) {
            progressionList = progressionList.concat(i);
        }
        let skillLevelBought = buySkillLevel(progressionList, p);
        setAttrs({ charisma : r+c+g+v+m+skillLevelBought });
    
    });
    });
    
    on('change:life_energy', function updateRawEnergy() {
    getAttrs(['life_energy'], function(values) {        
        let lifeForce = parseInt(values.life_energy) || 0;           
        let rawEnergy = 0;
        if  ( lifeForce < 100 ) {                                 
            rawEnergy = 0;
        } 
        else {
            rawEnergy = Math.floor(( lifeForce - 100 ) / 10 +1); 
        }        
            setAttrs({
            raw_energy: rawEnergy
        });
    });
    });
    
    on('change:dexterity change:armor_defense change:shields_defense change:psi_deflection', function updateRangedDefense() {
    getAttrs(['dexterity','armor_defense','shields_defense','psi_deflection'], function(values) {        
        let dex = parseInt(values.dexterity) || 0;           
        let armorDef = parseInt(values.armor_defense) || 0;
        let shieldsDef = parseInt(values.shields_defense) || 0;
        let psiDef_ranged = parseInt(values.psi_deflection) || 0;
            
            setAttrs({
            ranged_defense: 10 + dex + armorDef + shieldsDef + psiDef_ranged
        });
    });
    });
    
    on('change:dexterity change:armor_defense change:shields_defense change:psi_deflection', function updateMeleeDefense() {
        getAttrs(['dexterity','armor_defense','shields_defense','psi_deflection'], function(values) {        
            let dex = parseInt(values.dexterity) || 0;           
            let armorDef = parseInt(values.armor_defense) || 0;
            let shieldsDef = parseInt(values.shields_defense) || 0;
            let psiDef_melee = parseInt(values.psi_deflection) || 0;
                
                setAttrs({ melee_defense: dex + armorDef + shieldsDef + psiDef_melee });
        });
        });
    
    on('change:wisdom change:psi_clairvoyance change:psi_deflection change:psi_domination change:raw_energy', function updateMentalDefense() {
    getAttrs(['wisdom', 'psi_clairvoyance', 'psi_deflection', 'psi_domination', 'raw_energy'], function(values) {        
        let wis = parseInt(values.wisdom) || 0;
        let psi_cla = parseInt(values.psi_clairvoyance) || 0;
        let psi_def = parseInt(values.psi_deflection) || 0;
        let psi_dom = parseInt(values.psi_domination) || 0;
        let re = parseInt(values.raw_energy) || 0;
            
            setAttrs({
            mental_defense: 10 + wis + psi_cla + psi_def + psi_dom + Math.floor(re/10)
        });
    });
    });
    
    on('change:strength change:dexterity change:constitution change:intelligence change:wisdom change:charisma change:armor_defense change:shields_defense change:life_energy change:raw_energy change:psi_deflection change:psi_domination', function(eventInfo) {
    updateActionPoints();
    updateDefense();
    });
    
    
    
    // End of the primary attribute watchers, now comes other watchers.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       
    
    
    
    // Racial watcher
    on('change:race', function(eventInfo) {
    /*
        This watcher looks for changes in race, and set the primaryRacial 
        attributes as appropriate. Again, this code could be made more compact, 
        but I prefer readability over fewer lines of code. 
    */
    getAttrs(['race'], function(values) {
        if (values.race == "Kholodan") {
            setAttrs({ strengthRacial :         3 });
            setAttrs({ dexterityRacial :        0 });
            setAttrs({ constitutionRacial :     3 });
            setAttrs({ intelligenceRacial :     0 });
            setAttrs({ wisdomRacial :           0 });
            setAttrs({ charismaRacial :         0 });
            setAttrs({ strengthRacial_max :     25 });
            setAttrs({ dexterityRacial_max :    3 });
            setAttrs({ constitutionRacial_max : 15 });
            setAttrs({ racialHPBonus :          50 });
            setAttrs({ racialBaseLC :          15 });
        }
        else if (values.race == "Sollox") {
            setAttrs({ strengthRacial :         0 });
            setAttrs({ dexterityRacial :        0 });
            setAttrs({ constitutionRacial :     0 });
            setAttrs({ intelligenceRacial :     3 });
            setAttrs({ wisdomRacial :           3 });
            setAttrs({ charismaRacial :         0 });
            setAttrs({ strengthRacial_max :     3 });
            setAttrs({ dexterityRacial_max :    6 });
            setAttrs({ constitutionRacial_max : 5 });
            setAttrs({ racialHPBonus :          15 });
            setAttrs({ racialBaseLC :          20 });
        }
        else if (values.race == "Talaxian") {
            setAttrs({ strengthRacial :         2 });
            setAttrs({ dexterityRacial :        0 });
            setAttrs({ constitutionRacial :     3 });
            setAttrs({ intelligenceRacial :     0 });
            setAttrs({ wisdomRacial :           0 });
            setAttrs({ charismaRacial :         1 });
            setAttrs({ strengthRacial_max :     10 });
            setAttrs({ dexterityRacial_max :    6 });
            setAttrs({ constitutionRacial_max : 5 });
            setAttrs({ racialHPBonus :          25 });
            setAttrs({ racialBaseLC :          20 });
        }
        else if (values.race == "Thorn") {
            setAttrs({ strengthRacial :         0 });
            setAttrs({ dexterityRacial :        0 });
            setAttrs({ constitutionRacial :     1 });
            setAttrs({ intelligenceRacial :     1 });
            setAttrs({ wisdomRacial :           2 });
            setAttrs({ charismaRacial :         2 });
            setAttrs({ strengthRacial_max :     10 });
            setAttrs({ dexterityRacial_max :    3 });
            setAttrs({ constitutionRacial_max : 5 });
            setAttrs({ racialHPBonus :          20 });
            setAttrs({ racialBaseLC :          20 });
        }
        else {
            setAttrs({ strengthRacial :         0 });
            setAttrs({ dexterityRacial :        0 });
            setAttrs({ constitutionRacial :     0 });
            setAttrs({ intelligenceRacial :     0 });
            setAttrs({ wisdomRacial :           0 });
            setAttrs({ charismaRacial :         0 });
            setAttrs({ racialHPBonus :          20 });
            setAttrs({ racialBaseLC :          10 });
        }
    });
    });
    // End of racial watcher
    
    
    
    
    // Education and general skills watcher
    let listOfSkills = ['biology', 'business', 'chemistry', 'engineering', 'tronics', 'law', 'physics', 'programming', 'acrobatics', 'athletics', 'awareness', 
    'barter', 'charm', 'diplomacy', 'fighterPilot', 'focus', 'forgery', 'gatherInfo', 'insight', 'intimidate', 'investigation', 'medic', 'melee', 'perform', 
    'pilot', 'sleightOfHand', 'stealth', 'survival'];
    let listOfSkillsMap = listOfSkills.map(skill => 
    " change:" + skill + "Attribute" + 
    " change:" + skill + "VI" + 
    " change:" + skill + "Cause" + 
    " change:" + skill + "Misc" +
    " change:" + skill + "Progression" + 
    " change:" + skill + "Points").join(" ");
    listOfSkillsMap = listOfSkillsMap.concat("change:strength change:dexterity change:constitution change:intelligence change:wisdom change:charisma");
    on(listOfSkillsMap, (eventInfo) => {
    // GET VALUES
    let list = [];
    list = list.concat( listOfSkills.map(x => x + "VI") );
    list = list.concat( listOfSkills.map(x => x + "Cause") );
    list = list.concat( listOfSkills.map(x => x + "Misc") );
    list = list.concat( listOfSkills.map(x => x + "Points") );
    list = list.concat( listOfSkills.map(x => x + "Attribute") );
    list = list.concat( listOfSkills.map(x => x + "Progression") );
    getAttrs(list, (values) => {
        updatePointsSpent();
        listOfSkills.forEach(skill =>{
            // Get primary attribute
            let a = 0;
            let primaryAttribute = values[skill + "Attribute"].toLowerCase();
            getAttrs([primaryAttribute], primaryAttributeValue => {
                a = parseInt(primaryAttributeValue[primaryAttribute]) || 0;
                // Get secondary attributes
                let v = parseInt(values[skill + "VI"]) || 0;
                let c = parseInt(values[skill + "Cause"]) || 0;
                let m = parseInt(values[skill + "Misc"]) || 0;
                let progression = values[skill + "Progression"] || "";
                let p = parseInt(values[skill + "Points"]) || 0;
                
                // Time to parse how many skill points the points yield
                
                let progressionList = [];
                if (progression == "1,2,3,4,5...") { 
                    for (i = 1; i < 100; i++) {
                        progressionList = progressionList.concat(i);
                    }
                }
                if (progression != "1,2,3,4,5...")  { 
                    progressionList = progression.split(",");
                }
                let skillLevelBought = buySkillLevel(progressionList, p);
                let total = a+v+c+m+skillLevelBought;
                // Set attribute
                setAttrs({ [skill] :         total });
    
            });
        }); 
    });
    });
    
    
    
    // Watching for when sheet is opened and initializing any empty numerical values
    on('sheet:opened', function(values) {
    /* 
        The purpose of this watcher is to initialize all the 
        various important attributes, and thus avoid the complication of trying to 
        do something with something that is undefined. Other places of the code
        will try to have safeguards against this, but this is just another layer
        of protection in case I miss something. 
    */ 
    
    // INITIALIZING NUMERIAL ATTRIBUTES
    
    let listOfNumericalAttributes = new Object();
    
    // Core attributes
    listOfNumericalAttributes["strength"] = ["strengthRacial", "strengthCybernetic", "strengthVI", "strengthGenetic", "strengthMisc", "strengthPoints"];
    listOfNumericalAttributes["dexterity"] = ["dexterityRacial", "dexterityCybernetic", "dexterityVI", "dexterityGenetic", "dexterityMisc", "dexterityPoints"];
    listOfNumericalAttributes["constitution"] = ["constitutionRacial", "constitutionCybernetic", "constitutionVI", "constitutionGenetic", "constitutionMisc", "constitutionPoints"];
    listOfNumericalAttributes["intelligence"] = ["intelligenceRacial", "intelligenceCybernetic", "intelligenceVI", "intelligenceGenetic", "intelligenceMisc", "intelligencePoints"];
    listOfNumericalAttributes["wisdom"] = ["wisdomRacial", "wisdomCybernetic", "wisdomVI", "wisdomGenetic", "wisdomMisc", "wisdomPoints"];
    listOfNumericalAttributes["charisma"] = ["charismaRacial", "charismaCybernetic", "charismaVI", "charismaGenetic", "charismaMisc", "charismaPoints"];
    listOfNumericalAttributes["life_energy"] = ["attr_life_energy"];
    listOfNumericalAttributes["raw_energy"] = ["attr_raw_energy"];
    
    //Psionics
    listOfNumericalAttributes["psi_body_discipline"] = ["psi_domination_artifact", "psi_domination_artifact_quirk_bonus", "psi_domination_artifact_points", ];
    listOfNumericalAttributes["psi_causation"] = ["psi_causation_artifact", "psi_causation_quirk_bonus", "psi_causation_points", ];
    listOfNumericalAttributes["psi_clairvoyance"] = ["psi_clairvoyance_artifact", "psi_clairvoyance_quirk_bonus", "psi_clairvoyance_points", ];
    listOfNumericalAttributes["psi_deflection"] = ["psi_deflection_artifact", "psi_deflection_quirk_bonus", "psi_deflection_points", ];
    listOfNumericalAttributes["psi_domination"] = ["psi_domination_artifact", "psi_domination_quirk_bonus", "psi_domination_points", ];
    listOfNumericalAttributes["psi_energy_communion"] = ["psi_energy_communion_artifact", "psi_energy_communion_quirk_bonus", "psi_energy_communion_points", ];
    listOfNumericalAttributes["psi_molecular_alteration"] = ["psi_molecular_alteration_artifact", "psi_molecular_alteration_quirk_bonus", "psi_molecular_alteration_points", ];
    listOfNumericalAttributes["psi_pyrokinesis"] = ["psi_pyrokinesis_artifact", "psi_pyrokinesis_quirk_bonus", "psi_pyrokinesis_points", ];
    listOfNumericalAttributes["psi_subatomic_alteration"] = ["psi_subatomic_alteration_artifact", "psi_subatomic_alteration_quirk_bonus", "psi_subatomic_alteration_points", ];
    listOfNumericalAttributes["psi_telekinesis"] = ["psi_telekinesis_artifact", "psi_telekinesis_quirk_bonus", "psi_telekinesis_points", ];
    listOfNumericalAttributes["psi_anti_psionics"] = ["psi_anti_psionics_artifact", "psi_anti_psionics_quirk_bonus", "psi_anti_psionics_points", ];
    
    // Education
    listOfNumericalAttributes["biology"] = ["biologyVI", "biologyCause", "biologyMisc", "biologyPoints"];
    listOfNumericalAttributes["business"] = ["businessVI", "businessCause", "businessMisc", "businessPoints"];
    listOfNumericalAttributes["chemistry"] = ["chemistryVI", "chemistryCause", "chemistryMisc", "chemistryPoints"];
    listOfNumericalAttributes["engineering"] = ["engineeringVI", "engineeringCause", "engineeringMisc", "engineeringPoints"];
    listOfNumericalAttributes["tronics"] = ["tronicsVI", "tronicsCause", "tronicsMisc", "tronicsPoints"];
    listOfNumericalAttributes["law"] = ["lawVI", "lawCause", "lawMisc", "lawPoints"];
    listOfNumericalAttributes["physics"] = ["physicsVI", "physicsCause", "physicsMisc", "physicsPoints"];
    listOfNumericalAttributes["programming"] = ["programmingVI", "programmingCause", "programmingMisc", "programmingPoints"];
    
    // General skills
    listOfNumericalAttributes["acrobatics"] = ["acrobaticsVI", "acrobaticsCause", "acrobaticsMisc", "acrobaticsPoints"];
    listOfNumericalAttributes["athletics"] = ["athleticsVI", "athleticsCause", "athleticsMisc", "athleticsPoints"];
    listOfNumericalAttributes["awareness"] = ["awarenessVI", "awarenessCause", "awarenessMisc", "awarenessPoints"];
    listOfNumericalAttributes["barter"] = ["barterVI", "barterCause", "barterMisc", "barterPoints"];
    listOfNumericalAttributes["charm"] = ["charmVI", "charmCause", "charmMisc", "charmPoints"];
    listOfNumericalAttributes["diplomacy"] = ["diplomacyVI", "diplomacyCause", "diplomacyMisc", "diplomacyPoints"];
    listOfNumericalAttributes["fighterPilot"] = ["fighterPilotVI", "fighterPilotCause", "fighterPilotMisc", "fighterPilotPoints"];
    listOfNumericalAttributes["focus"] = ["focusVI", "focusCause", "focusMisc", "focusPoints"];
    listOfNumericalAttributes["forgery"] = ["forgeryVI", "forgeryCause", "forgeryMisc", "forgeryPoints"];
    listOfNumericalAttributes["gatherInfo"] = ["gatherInfoVI", "gatherInfoCause", "gatherInfoMisc", "gatherInfoPoints"];
    listOfNumericalAttributes["insight"] = ["insightVI", "insightCause", "insightMisc", "insightPoints"];
    listOfNumericalAttributes["intimidate"] = ["intimidateVI", "intimidateCause", "intimidateMisc", "intimidatePoints"];
    listOfNumericalAttributes["investigation"] = ["investigationVI", "investigationCause", "investigationMisc", "investigationPoints"];
    listOfNumericalAttributes["medic"] = ["medicVI", "medicCause", "medicMisc", "medicPoints"];
    listOfNumericalAttributes["melee"] = ["meleeVI", "meleeCause", "meleeMisc", "meleePoints"];
    listOfNumericalAttributes["perform"] = ["performVI", "performCause", "performMisc", "performPoints"];
    listOfNumericalAttributes["pilot"] = ["pilotVI", "pilotCause", "pilotMisc", "pilotPoints"];
    listOfNumericalAttributes["sleightOfHand"] = ["sleightOfHandVI", "sleightOfHandCause", "sleightOfHandMisc", "sleightOfHandPoints"];
    listOfNumericalAttributes["stealth"] = ["stealthVI", "stealthCause", "stealthMisc", "stealthPoints"];
    listOfNumericalAttributes["survival"] = ["survivalVI", "survivalCause", "survivalMisc", "survivalPoints"];
    
    // Create a list of all the variables and populate it with the values above
    let list = [];
    
    for (var key in listOfNumericalAttributes) {
        // check if the property/key is defined in the object itself, not in parent
        list.push(key);
        if (listOfNumericalAttributes.hasOwnProperty(key)) {           
            list = list.concat(listOfNumericalAttributes[key]);
        } 
    }
    
    // Initialize all of the undefined values on the character sheet to 0. 
    getAttrs(list, function(values) {
        list.forEach(attribute =>{
            if (!values[attribute]) {
                setAttrs({ [attribute] : 0 });
            }
        });
    });
    });
    
    
    
    // Watching for when armor and shields change
    on('change:penetrationPlus change:armorPenetrationPlus change:shieldsPenetrationPlus change:armor_defense change:shields_defense change:life_energy change:psi_deflection change:psi_domination', function(eventInfo) {
    updateDefense();
    });
    
    
    // Watching for when rangedweapon change
    on('change:rangedWeapon change:rangedDamage change:rangedHitBonus change:rangedPenetrationPlus change:rangedRoF change:rangedAmmunition change:rangedAmmunition_max change:rangedRange change:rangedSkill', function(eventInfo) {
    getAttrs(['rangedWeapon', 'rangedDamage', 'rangedHitBonus', 'rangedPenetrationPlus', "rangedRoF", "rangedAmmunition", 'rangedAmmunition_max', 'rangedRange', 'rangedSkill',], function(values) {
        let textToChat = "&{template:default}"; 
        textToChat = textToChat.concat(" {{name=" + values.rangedWeapon + "}}");
        textToChat = textToChat.concat(" {{Hit= [[1d20+" + values.rangedHitBonus + "]]}}");
        textToChat = textToChat.concat(" {{Damage= [[" + values.rangedDamage + "]]}}");
        textToChat = textToChat.concat(" {{Penetration= " + values.rangedPenetrationPlus + "}}");
        textToChat = textToChat.concat(" {{Ammunition= " + String(parseInt(values.rangedAmmunition)-1) + " / " + values.rangedAmmunition_max + "}}");
        console.log(textToChat);
        setAttrs({ rangedRoll :         textToChat });
    });
    });
    
    on('clicked:rangedAmmoReduce', function() {
    getAttrs(['rangedAmmunition'], function(values) {
        setAttrs({ rangedAmmunition: values.rangedAmmunition-1 });
    });
    });
    
    // Watching for when rangedweapon2 changes
    on('change:rangedWeapon2 change:rangedDamage2 change:rangedHitBonus2 change:rangedPenetrationPlus2 change:rangedRoF2 change:rangedAmmunition2 change:rangedAmmunition_max2 change:rangedRange2 change:rangedSkill2', function(eventInfo) {
    getAttrs(['rangedWeapon2', 'rangedDamage2', 'rangedHitBonus2', 'rangedPenetrationPlus2', "rangedRoF2", "rangedAmmunition2", 'rangedAmmunition_max2', 'rangedRange2', 'rangedSkill2',], function(values) {
        let textToChat = "&{template:default}"; 
        textToChat = textToChat.concat(" {{name=" + values.rangedWeapon2 + "}}");
        textToChat = textToChat.concat(" {{Hit= [[1d20+" + values.rangedHitBonus2 + "]]}}");
        textToChat = textToChat.concat(" {{Damage= [[" + values.rangedDamage2 + "]]}}");
        textToChat = textToChat.concat(" {{Penetration= " + values.rangedPenetrationPlus2 + "}}");
        textToChat = textToChat.concat(" {{Ammunition= " + String(parseInt(values.rangedAmmunition2)-1) + " / " + values.rangedAmmunition_max2 + "}}");
        console.log(textToChat);
        setAttrs({ rangedRoll2 :         textToChat });
    });
    });
    
    on('clicked:rangedAmmoReduce', function() {
    getAttrs(['rangedAmmunition'], function(values) {
        setAttrs({ rangedAmmunition: values.rangedAmmunition-1 });
    });
    });
    
    on('clicked:rangedAmmoReload', function() {
    getAttrs(['rangedAmmunition', 'rangedAmmunition_max'], function(values) {
        let maximum = values.rangedAmmunition_max;
        setAttrs({ rangedAmmunition: maximum });
    });
    });
    
    on('clicked:rangedAmmoReduce2', function() {
    getAttrs(['rangedAmmunition2'], function(values) {
        setAttrs({ rangedAmmunitio2: values.rangedAmmunition2-1 });
    });
    });
    
    on('clicked:rangedAmmoReload2', function() {
    getAttrs(['rangedAmmunition2', 'rangedAmmunition_max2'], function(values) {
        let maximum = values.rangedAmmunition_max2;
        setAttrs({ rangedAmmunition2: maximum });
    });
    });
    
    
    // Watching for when melee weapon change
    on('change:meleeWeapon change:meleeDamage change:meleeHitBonus change:meleePenetrationPlus change:meleeRoF change:meleeAmmunition change:meleeAmmunition_max change:meleeRange change:meleeSkill', function(eventInfo) {
    getAttrs(['meleeWeapon', 'meleeDamage', 'meleeHitBonus', 'meleePenetrationPlus', "meleeRoF", "meleeAmmunition", 'meleeAmmunition_max', 'meleeRange', 'meleeSkill'], function(values) {
        let textToChat = "&{template:default}"; 
        textToChat = textToChat.concat(" {{name=" + values.meleeWeapon + "}}");
        textToChat = textToChat.concat(" {{Hit= [[1d20+" + values.meleeHitBonus + "]]}}");
        textToChat = textToChat.concat(" {{Damage= [[" + values.meleeDamage + "]]}}");
        textToChat = textToChat.concat(" {{Penetration= " + values.meleePenetrationPlus + "}}");
        textToChat = textToChat.concat(" {{Ammunition= " + String(parseInt(values.meleeAmmunition)-1) + " / " + values.meleeAmmunition_max + "}}");
        console.log(textToChat);
        setAttrs({ meleeRoll :         textToChat });
    });
    });
    
    
    // Watching for when meleeweapon2 change
    on('change:meleeWeapon2 change:meleeDamage2 change:meleeHitBonus2 change:meleePenetrationPlu2s change:meleeRoF2 change:meleeAmmunition2 change:meleeAmmunition_max2 change:meleeRange2 change:meleeSkill2', function(eventInfo) {
    getAttrs(['meleeWeapon2', 'meleeDamage2', 'meleeHitBonus2', 'meleePenetrationPlus2', "meleeRoF", "meleeAmmunition", 'meleeAmmunition_max2', 'meleeRange2', 'meleeSkill'], function(values) {
        let textToChat = "&{template:default}"; 
        textToChat = textToChat.concat(" {{name=" + values.meleeWeapon2 + "}}");
        textToChat = textToChat.concat(" {{Hit= [[1d20+" + values.meleeHitBonus2 + "]]}}");
        textToChat = textToChat.concat(" {{Damage= [[" + values.meleeDamage2 + "]]}}");
        textToChat = textToChat.concat(" {{Penetration= " + values.meleePenetrationPlus2 + "}}");
        textToChat = textToChat.concat(" {{Ammunition= " + String(parseInt(values.meleeAmmunition2)-1) + " / " + values.meleeAmmunition_max2 + "}}");
        console.log(textToChat);
        setAttrs({ meleeRoll2 :         textToChat });
    });
    });
    
    on('clicked:meleeAmmoReduce', function() {
    getAttrs(['meleeAmmunition'], function(values) {
        // get the level stat, and coerce into a numerical value.
        setAttrs({ meleeAmmunition: values.meleeAmmunition-1 });
    });
    });
    
    on('clicked:meleeAmmoReload', function() {
    getAttrs(['meleeAmmunition', 'meleeAmmunition_max'], function(values) {
        // get the level stat, and coerce into a numerical value.
        let maximum = values.meleeAmmunition_max;
        setAttrs({ meleeAmmunition: maximum });
    });
    });
    
    on('clicked:meleeAmmoReduce2', function() {
    getAttrs(['meleeAmmunition2'], function(values) {
        // get the level stat, and coerce into a numerical value.
        setAttrs({ meleeAmmunition2: values.meleeAmmunition2-1 });
    });
    });
    
    on('clicked:meleeAmmoReload2', function() {
    getAttrs(['meleeAmmunition2', 'meleeAmmunition_max2'], function(values) {
        // get the level stat, and coerce into a numerical value.
        let maximum = values.meleeAmmunition_max2;
        setAttrs({ meleeAmmunition2: maximum });
    });
    });
    
    
    
    
    </script>
